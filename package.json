{
  "name": "rails-tests",
  "displayName": "Rails Tests",
  "description": "Rails tests enables a code lens to run your tests quickly from within your test file.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "repository": "https://github.com/jbosse/vscode-rails-test",
  "publisher": "jobo-studios",
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:ruby"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rails.lens.test.run",
        "title": "Run Tests"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.56.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
